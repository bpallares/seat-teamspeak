  "type": "seat-plugin",
  "license": "GPL-3.0-or-later",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Warlof Tutsimo",
      "email": "loic.leuilliot@gmail.com"
    },
    {
      "name": "Denngarr B'tarn",
      "email": "ed.stafford@gmail.com"
    },
    {
      "name": "hderp886",
      "email": "hderp886@gmail.com"
    }
